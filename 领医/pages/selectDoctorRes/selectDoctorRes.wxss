page {
  background-color: RGBA(249, 249, 249, 1);
  padding-bottom: 150rpx;
}

.headGroup {
  width: 100%;
  height: 94rpx;
  background-color: rgba(255, 255, 255, 1);
  display: flex;
  align-items: center;
  justify-content: space-around;
}

.headItem {
  width: 81rpx;
  height: 81rpx;
  border-radius: 50%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 25rpx;
  /* font-weight: bold; */
  color: rgba(153, 153, 153, 1);
}

.active {
  background-color: rgba(99, 223, 197, 1);
  color: rgba(255, 255, 255, 1);
}

.day, .date {
}

.item {
  width: 100%;
  background-color: rgba(255, 255, 255, 1);
  /* margin-top: 20rpx; */
  box-sizing: border-box;
  padding: 0rpx 30rpx;
}

.itemHead {
  padding: 26rpx 0rpx;
  height: 144rpx;
  width: 100%;
  box-sizing: border-box;
  border-bottom: 1rpx solid rgba(0, 0, 0, 0.06);
  display: flex;
}

.headLogo {
  width: 82rpx;
  height: 82rpx;
  /* background-color: red; */
  border-radius: 50%;
}

.headTitle {
  width: 350rpx;
  height: auto;
  margin-left: 22rpx;
}

.bottonGroup {
  width: 160rpx;
  height: 52rpx;
  border-radius: 26rpx;
  border: 1rpx solid RGBA(99, 223, 197, 1);
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  font-family: PingFang-SC-Bold;
  color: rgba(99, 223, 197, 1);
  margin-left: auto;
}

.title {
  font-size: 26rpx;
  font-weight: normal;
  color: rgba(51, 51, 51, 1);
}

.minTitle:first-child {
  margin-left: 40rpx;
}

.minTitle {
  font-size: 22rpx;
  font-family: PingFang-SC-Bold;
  color: rgba(153, 153, 153, 1);
  margin-right: 20rpx;
}

.adder {
  margin-top: 14rpx;
  font-size: 22rpx;
  font-family: PingFang-SC-Bold;
  color: rgba(153, 153, 153, 1);
}

.itemOther {
  width: 100%;
  height: 104rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1rpx solid rgba(0, 0, 0, 0.06);
}
.itemOtherList{
    width: 100%;
  height: 104rpx;
  display: flex;
  align-items: center;
  /* justify-content: space-between; */
}

.otherHead {
  font-size: 26rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  text-indent: 20rpx;
}

.otherData {
  font-size: 22rpx;
  font-family: PingFang-SC-Bold;
  /* font-weight: bold; */
  color: rgba(153, 153, 153, 1);
}
.otherData .iconfont{
   font-size: 22rpx;
}
.otherData span {
  margin-left: 20rpx;
}

.tip {
  width: 100%;
  height: auto;
  padding: 0rpx 50rpx;
  box-sizing: border-box;
  margin-top: 100rpx;
}

.tipTitle {
  font-size: 26rpx;
  font-weight: bold;
  color: rgba(153, 153, 153, 1);
}

.tipContent {
  font-size: 24rpx;
  color: rgba(218, 218, 218, 1);
  margin-top: 30rpx;
  line-height: 40rpx;
}

.sure {
  width: 450rpx;
  height: 100rpx;
  background-color: rgba(99, 223, 197, 1);
  font-size: 28rpx;
  color: rgba(249, 249, 249, 1);
}
.sureWrap{
  text-align: center;
  line-height: 100rpx;
  width:100%;
  height: 100rpx;
  background-color: #f9f9f9;
  position: fixed;
  bottom: 0;
  right: 0;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  font-size: 28rpx;
  color: rgba(249, 249, 249, 1);
}

.allTime{
  width: 100%;
  height: 60rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  padding: 0rpx 30rpx 0rpx 50rpx;
  font-size:26rpx;
font-family:PingFang-SC-Regular;
font-weight:400;
color:rgba(153,153,153,1);
}
.clickActive{
  background-color: rgba(241,241,241,1);
}
.corR{
  color: rgba(228, 84, 135, 1)
}
.othertip{
  justify-self:flex-start;
  font-size:22rpx;
font-family:PingFang-SC-Bold;
color:rgba(228,84,135,1);
margin-left: 30rpx;
}