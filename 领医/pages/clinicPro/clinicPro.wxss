page {
  /* position: relative; */
  width: 100%;
  /* overflow: hidden; */
  background-color: RGBA(249, 249, 249, 1);
}

.slide-image {
  width: 100%;
  height: 422rpx;
}

.res {
  overflow: hidden;
  background-color: rgba(255, 255, 255, 1);
  padding: 0rpx 0rpx 45rpx 0rpx;
}

.res_list {
  font-size: 32rpx;
  font-family: Adobe Heiti Std R;
  font-weight: normal;
  color: rgba(102, 102, 102, 1);
  box-sizing: border-box;
  padding: 0rpx 50rpx;
  margin-top: 60rpx;
  display: flex;
}

.res_list .iconfont {
  color: RGBA(100, 223, 197, 1);
  margin-right: 30rpx;
}

.ks {
  padding: 25rpx 30rpx 12rpx 30rpx;
  background-color: rgba(255, 255, 255, 1);
  margin-top: 30rpx;
}

.title {
  font-size: 32rpx;
  font-weight: normal;
  color: rgba(51, 51, 51, 1);
}

.taglist {
  margin-top: 23rpx;
  display: flex;
  flex-wrap: wrap;
}

.tag {
  width: 148rpx;
  height: 50rpx;
  border: 2rpx solid rgba(230, 230, 230, 1);
  border-radius: 25rpx;
  font-size: 28rpx;
  font-weight: normal;
  color: rgba(153, 153, 153, 1);
  display: flex;
  align-items: center;
  justify-content: center;
  margin-right: 26rpx;
  margin-top: 17rpx;
 background:linear-gradient(180deg,rgba(246,246,246,1) 0%,rgba(233,233,233,1) 100%);
}

.tag:nth-child(4n) {
  margin-right: 0rpx;
}

.produce {
  font-size: 28rpx;
  font-family: PingFang-SC-Bold;
  color: rgba(153, 153, 153, 1);
  line-height: 47rpx;
  margin-top: 30rpx;
}

.mytip{
  font-size:24rpx;
font-family:PingFang-SC-Bold;
font-weight:bold;
color:rgba(153,153,153,1);
margin-left: 20rpx;
}
.more{
  width: 100%;
  height: auto;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  margin-top: 33rpx;
}
.more span{
  font-size:22rpx;
font-weight:normal;
color:rgba(153,153,153,1);
}
.icon-iconfontarrows{
    font-size:22rpx;
font-weight:normal;
color:rgba(153,153,153,1);
}