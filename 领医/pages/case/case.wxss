.wrap {
  height: 100%;
  box-sizing: border-box;
  padding: 0rpx 25rpx;
}

.item_head {
  width: 100%;
  height: 168rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid rgba(0, 0, 0, 0.06);
  display: flex;
  align-items: center;
}

.item {
  width: 100%;
  height: 137rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid rgba(0, 0, 0, 0.06);
  display: flex;
  align-items: center;
}

.avater {
  width: 81rpx;
  height: 81rpx;
  background-color: red;
  border-radius: 50%;
}

.userRes {
  width: 350rpx;
  height: auto;
  margin-left: 20rpx;
}

.name {
  font-size: 30rpx;
  /* font-family:Adobe Heiti Std R; */
  font-weight: normal;
  color: rgba(51, 51, 51, 1);
}

.age, .sex {
  font-size: 24rpx;
  font-weight: normal;
  color: rgba(102, 102, 102, 1);
}

.change {
  display: flex;
  align-items: center;
  justify-content: flex-end;
  width: 200rpx;
  margin-left: auto;
  font-size: 24rpx;
  /* font-family:Adobe Heiti Std R; */
  font-weight: normal;
  color: rgba(153, 153, 153, 1);
}

.buttonGroup {
  width: 200rpx;
  display: flex;
  margin-left: auto;
  flex-direction: column;
  align-items: flex-end;
  justify-content: space-between;
}

.tolist {
  width: 14rpx;
  height: 22rpx;
  background-color: red;
  margin-left: 12rpx;
}

.zw {
  font-size: 22rpx;
  font-family: PingFang;
  /* font-weight:bold; */
  color: rgba(153, 153, 153, 1);
  margin-left: 8rpx;
}

.zw:first-child {
  margin-left: 45rpx;
}

.res {
  margin-top: 10rpx;
  font-size: 22rpx;
  font-family: PingFang-SC-Bold;
  /* font-weight:bold; */
  color: rgba(153, 153, 153, 1);
}

.date {
  font-size: 22rpx;
  font-family: PingFang-SC-Bold;
  /* font-weight:bold; */
  color: rgba(153, 153, 153, 1);
}

.lookCase {
  width: 160rpx;
  height: 52rpx;
  box-sizing: border-box;
  border: 2rpx solid rgba(99, 223, 197, 1);
  border-radius: 26rpx;
  font-size: 24rpx;
  font-family: PingFang-SC-Bold;
  /* font-weight:bold; */
  color: rgba(99, 223, 197, 1);
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 10rpx;
}
.icon-jiantou{
  font-size: 24rpx;
  color: #999999;
  margin-left: 12rpx;
}