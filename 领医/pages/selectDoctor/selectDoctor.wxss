page {
  background-color: RGBA(249, 249, 249, 1);
}

.headGroup {
  width: 100%;
  height: 94rpx;
  background-color: rgba(255, 255, 255, 1);
  display: flex;
  align-items: center;
  justify-content: space-around;
}

.headItem {
  width: 81rpx;
  height: 81rpx;
  border-radius: 50%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 26rpx;
  font-weight: bold;
  color: rgba(153, 153, 153, 1);
}

.active {
  background-color: rgba(99, 223, 197, 1);
  color: rgba(255, 255, 255, 1);
}

.day, .date {
}

.item {
  width: 100%;
  height: 250rpx;
  background-color: rgba(255, 255, 255, 1);
  margin-top: 20rpx;
  box-sizing: border-box;
  padding: 0rpx 30rpx;
}

.itemHead {
  padding: 26rpx 0rpx;
  height: 144rpx;
  width: 100%;
  box-sizing: border-box;
  border-bottom: 1rpx solid rgba(0, 0, 0, 0.06);
  display: flex;
}

.headLogo {
  width: 82rpx;
  height: 82rpx;
  /* background-color: red; */
  border-radius: 50%;
}

.headTitle {
  width: 350rpx;
  height: auto;
  margin-left: 22rpx;
}

.bottonGroup {
  width: 160rpx;
  height: 52rpx;
  border-radius: 26rpx;
  border: 1rpx solid RGBA(99, 223, 197, 1);
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  font-family: PingFang-SC-Bold;
  color: rgba(99, 223, 197, 1);
  margin-left: auto;
}

.title {
  font-size: 26rpx;
  font-weight: normal;
  color: rgba(51, 51, 51, 1);
}

.minTitle:first-child {
  margin-left: 40rpx;
}

.minTitle {
  font-size: 22rpx;
  font-family: PingFang-SC-Bold;
  color: rgba(153, 153, 153, 1);
  margin-right: 20rpx;
}

.adder {
  margin-top: 24rpx;
  font-size: 22rpx;
  font-family: PingFang-SC-Bold;
  color: rgba(153, 153, 153, 1);
}

.itemOther {
  width: 100%;
  height: 104rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.otherHead {
  font-size: 26rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  text-indent: 20rpx;
}

.otherData {
  font-size: 22rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(153, 153, 153, 1);
}

.otherData span {
  margin-left: 20rpx;
}
