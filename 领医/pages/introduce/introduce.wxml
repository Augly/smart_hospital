<swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" class='slide-image' wx:if="{{imgUrls.length>2}}">
  <block wx:for="{{imgUrls}}" wx:key=''>
    <swiper-item class='slide-image'>
      <image src="{{item}}" class="slide-image" />
    </swiper-item>
  </block>
</swiper>
<block wx:for="{{imgUrls}}" wx:if="{{imgUrls.length<2}}" wx:key=''>
  <image wx:if="{{item!=''}}" src="{{item}}" class="slide-image" />
</block>
<view class='res'>
  <view class='res_list'>
    <span class='iconfont icon-yiyuan1'></span>{{product.organization_name}}</view>
  <view class='res_list'>
    <span class='iconfont icon-dianhua'></span>联系电话(手机):{{product.organization_phone}}</view>
  <view class='res_list'>
    <span class='iconfont icon-weizhi'></span>{{product.organization_phone}}</view>
</view>


<view class='ks'>
  <view class='title'>科室</view>
  <view class='taglist'>
    <view class='tag' wx:for="{{ks}}" wx:for-item="item" wx:for-index="index">{{item.subjects_name}}</view>
  </view>
</view>

<view class='ks'>
  <view class='title'>关于我们</view>
  <view class='produce'>{{product.organization_introduce}}</view>
</view>