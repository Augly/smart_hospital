.list {
  box-sizing: border-box;
  width: 100%;
  height: auto;
  padding: 20rpx 30rpx;
  box-shadow: 0px 3rpx 18rpx 3rpx rgba(0, 0, 0, 0.11);
  display: flex;
  margin-top: 40rpx;
  /* align-items: center; */
}

.list_logo {
  width: 244rpx;
  height: 184rpx;
  background-color: red;
}

.list_Res {
  width: 260rpx;
  height: auto;
  /* background-color: blue;*/
  overflow: hidden;
  margin-left: 20rpx;
}

.title {
  font-size: 24rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.res {
  margin-top: 18rpx;
  font-size: 20rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(153, 153, 153, 1);
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 5;
  -webkit-box-orient: vertical;
}
.list_Other{
  /* width: 115rpx; */
  margin-left: auto;
}

.doctor_name {
  font-size: 26rpx;
  font-family: PingFang-SC-Bold;
  color: rgba(153, 153, 153, 1);
  margin-top: 48rpx;
}

.date {
  font-size: 20rpx;
  font-family: PingFang-SC-Bold;
  margin-top: 77rpx;
  color: rgba(153, 153, 153, 1);
}
