}

.star {
  width: 22rpx;
  height: 22rpx;
  background-color: #ffbf12;
  margin-right: 4rpx;
}

.gold {
  font-size: 26rpx;
  font-family: PingFang-SC-Bold;
  /* font-weight:bold; */
