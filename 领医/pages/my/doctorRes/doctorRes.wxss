/* pages/my/doctor/doctor.wxss */

page {
  background-color: rgba(252, 252, 252, 1);
}

.wrap {
  height: 100%;
}

.item {
  width: 100%;
  height: 215rpx;
  box-sizing: border-box;
  padding: 20rpx;
  background-color: #63dfc5;
  /* box-shadow: 0px 3rpx 18rpx 3rpx rgba(0, 0, 0, 0.11); */
  display: flex;
  justify-content: space-between;
}

.logo {
  width: 82rpx;
  height: 82rpx;
  /* background-color: red; */
  border-radius:50%; 
}
.itemlist .iconfont {
  font-size:38rpx;
  color: RGBA(100, 223, 197, 1);
}
.res {
  width: 550rpx;
  height: 100%;
  display: flex;
  justify-content: space-between;
}

.res_left {
  width: 360rpx;
  height: 100%;
  /* background-color: red; */
}

.res_right {
  width: 180rpx;
  height: 100%;
  /* background-color: blue; */
}

.d_name {
  font-size: 26rpx;
  font-family: PingFang-SC-Bold;
  line-height: 40rpx;
  color: #fafafa;
}

.d_k {
  line-height: 40rpx;
}

.ks {
  font-size: 22rpx;
  font-family: PingFang-SC-Bold;
  /* font-weight:bold; */
  color: #fafafa;
}

.z_do {
  font-size: 22rpx;
  font-family: PingFang-SC-Bold;
  /* font-weight:bold; */
  color: #f2f2f2;
  margin-left: 21rpx;
}

.adder {
  line-height: 40rpx;
  font-size: 22rpx;
  font-family: PingFang-SC-Bold;
  /* font-weight:bold; */
  color: #f2f2f2;
}

.buttonGroup {
  width: 100%;
  margin-top: 20rpx;
  /* height: 200rpx; */
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.look, .ask {
  width: 160rpx;
  height: 52rpx;
  box-sizing: border-box;
  border: 2rpx solid rgba(99, 223, 197, 1);
  border-radius: 26rpx;
  font-size: 24rpx;
  font-family: PingFang-SC-Bold;
  /* font-weight:bold; */
  display: flex;
  align-items: center;
  justify-content: center;
  color: rgba(99, 223, 197, 1);
}

.res_right_title {
  font-size: 20rpx;
  font-family: PingFang-SC-Bold;
  text-align: right;
  /* font-weight:bold; */
  color: #f1f1f1;
}

.starGroup {
  width: 100%;
  margin-top: 10rpx;
  display: flex;
  align-items: center;
}

.star {
  width: 22rpx;
  height: 22rpx;
  background-color: #ffbf12;
  margin-right: 4rpx;
}

.gold {
  font-size: 26rpx;
  font-family: PingFang-SC-Bold;
  /* font-weight:bold; */
  color: #fafafa;
}

.dataGroup {
  width: 100%;
  height: 140rpx;
  display: flex;
  align-items: center;
  background-color: rgba(255, 255, 255, 1);
}

.dataItem {
  width: 250rpx;
  height: 88rpx;
  box-sizing: border-box;
  border-right: 1rpx solid rgba(51, 51, 51, 0.1);
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}

.dataItem:last-child {
  border: none;
}

.dataNum {
  font-size: 38rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
}

.popr {
  font-size: 26rpx;
  font-family: PingFang-SC-Bold;
  /* font-weight:bold; */
  color: rgba(153, 153, 153, 1);
}

.dataTitle {
  font-size: 24rpx;
  font-family: PingFang-SC-Bold;
  /* font-weight:bold; */
  color: rgba(153, 153, 153, 1);
}

.button {
  display: flex;
  align-items: center;
  justify-content: center;
}

.ask {
  width: 200rpx;
  height: 65rpx;
  display: flex;
  align-items: center;
  border-radius: 32.5rpx;
}

.goodT {
  width: 100%;
  height: auto;
  margin-top: 30rpx;
  background-color: rgba(255, 255, 255, 1);
  box-sizing: border-box;
  padding: 20rpx 30rpx 50rpx 30rpx;
}

.goodTTitle {
  font-size: 32rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
}

.tagGroup {
  margin-top: 32rpx;
  padding-bottom: 50rpx;
}

.tag {
  padding: 11rpx 45rpx 11rpx 45rpx;
  height: 52rpx;
  line-height: 52rpx;
  font-size: 28rpx;
  font-family: PingFang-SC-Bold;
  /* font-weight:bold; */
  color: rgba(99, 223, 197, 1);

  border: 2rpx solid rgba(230, 230, 230, 1);
  border-radius: 52rpx;
  margin-right: 33rpx;
}

.describe{
  font-size:28rpx;
font-family:PingFang-SC;
/* font-weight:bold; */
color:rgba(153,153,153,1);
line-height:47rpx;
margin-top: 36rpx;

}

.itemlist{
  width: 100%;
  display: flex;
  margin-top: 30rpx;
}


.item_logo{
  width: 30rpx;
  height: 30rpx;
  background-color: red;
}

.item_res{
  margin-left: 30rpx;
  width: 600rpx;
  font-size:32rpx;
font-weight:normal;
color:rgba(153,153,153,1);
}
.iconfont{
  font-size: 24rpx;
  margin-right: 4rpx;
}

.icon-xingxing{
  color: RGBA(255, 191, 18, 1);
}
.icon-xingxingxian{
  color: rgba(0, 0, 0, 1)
}