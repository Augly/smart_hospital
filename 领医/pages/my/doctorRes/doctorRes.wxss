}

.dataGroup {
  width: 100%;
  height: 140rpx;
  display: flex;
  align-items: center;
  background-color: rgba(255, 255, 255, 1);
}

.dataItem {
  width: 220rpx;
  height: 88rpx;
  box-sizing: border-box;
  border-right: 1rpx solid rgba(51, 51, 51, 0.1);
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content:space-around;
}

.dataItem:last-child {
  border: none;
}

.dataNum {
  font-size: 38rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
}

.popr {
  font-size: 26rpx;
