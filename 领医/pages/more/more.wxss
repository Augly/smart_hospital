page {
  box-sizing: border-box;
  background-color: RGBA(236, 250, 250, 1);
  padding: 40rpx;
}

.head {
  width: 100%;
  height: auto;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.head_item {
  width: 170rpx;
  height: 100rpx;
  background: rgba(247, 206, 229, 0.6);
  border-radius: 10rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.item_title {
  font-size: 26rpx;
  font-family: MicrosoftYaHei;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
}

.item_mintitle {
  font-size: 20rpx;
  font-family: MicrosoftYaHei;
  /* font-weight:bold; */
  color: rgba(153, 153, 153, 1);
  margin-top: 6rpx;
}

.banner {
  width: 100%;
  height: 766rpx;
  margin-top: 55rpx;
  margin-bottom: 40rpx;
  /* height:auto !important; */
}

.list_head {
  width: 220rpx;
  height: 60rpx;
  background: rgba(247, 206, 229, 0.6);
  border-radius: 10rpx;
  text-align: center;
  line-height: 60rpx;
  font-size: 26rpx;
  font-family: MicrosoftYaHei;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
}

.list_content {
  box-sizing: border-box;
  padding: 20rpx 18rpx;
  font-size: 20rpx;
  font-family: MicrosoftYaHei;
  font-weight: bold;
  color: rgba(153, 153, 153, 1);
  text-align: justify;
  text-justify: inter-ideograph;
}

.go {
  width: 340rpx;
  height: 74rpx;
  background: rgba(99, 223, 197, 1);
  box-shadow: 0rpx 2rpx 16rpx 0rpx rgba(0, 0, 0, 0.2);
  border-radius: 30rpx;
  margin: 0 auto;
  margin-top: 40rpx;
  font-size: 34rpx;
  font-family: MicrosoftYaHei;
  font-weight: 400;
  text-align: center;
  line-height: 74rpx;
  color: rgba(255, 255, 255, 1);
}

.go .icon-jiantou {
  font-size: 34rpx;
  margin-left: 10rpx;
}
