.switchover {
  width: 100%;
}

.switchover-item {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 642rpx;
  height: auto;
  margin: 0rpx 54rpx;
  padding: 25rpx 0rpx;
  border-bottom: 1rpx solid rgba(0, 0, 0, 0.06);
}

.switchover-right {
  width: 500rpx;
}

.switchover-head {
  position: absolute;
  top: 0;
  width: 690rpx;
  margin: 0rpx 30rpx;
  padding: 34rpx 0rpx;
  overflow: hidden;
  border-bottom: 1rpx solid rgba(0, 0, 0, 0.06);
}

.content {
  margin-top: 210rpx;
  position: relative;
  height: 100%;
  display: flex;
}

.switchover-img {
  width: 215rpx;
  height: 142rpx;
  float: left;
}

.switchover-text {
  width: 454rpx;
  height: 142rpx;
  float: right;
}

.switchover-item-hover::before {
  content: '';
  position: absolute;
  left: 0;
  width: 7rpx;
  height: 97rpx;
  background-color: RGBA(228, 84, 135, 1);
}

.switchover-headline {
  height: 31rpx;
  line-height: 31rpx;
  font-size: 32rpx;
  font-family: Adobe Heiti Std R;
  font-weight: normal;
  color: rgba(51, 51, 51, 1);
}

.switchover-headline-head {
  margin-top: 36rpx;
}

.switchover-headline-s {
  display: inline;
  font-size: 20rpx;
  font-family: Adobe Heiti Std R;
  font-weight: normal;
  color: rgba(51, 51, 51, 1);
}

.switchover-subhead {
  height: 23rpx;
  line-height: 23rpx;
  font-size: 24rpx;
  font-family: Adobe Heiti Std R;
  font-weight: normal;
  color: rgba(153, 153, 153, 1);
  margin-top: 42rpx;
}

.switchover-subhead-head {
  margin-top: 34rpx;
}

.switchover-subhead-b {
  display: inline;
  font-weight: bold;
  color: #666;
}

.switchover-subhead-c {
  font-size: 24rpx;
  font-weight: normal;
  color: rgba(153, 153, 153, 1);
  text-indent: 30rpx;
}

.mintitle {
  font-size: 24rpx;
  font-weight: normal;
  color: rgba(153, 153, 153, 1);
}

.change {
  display: flex;
  align-items: center;
  justify-content: flex-end;
  width: 200rpx;
  margin-left: auto;
  font-size: 24rpx !important;
  line-height: 24rpx;
  /* font-family:Adobe Heiti Std R; */
  font-weight: normal;
  color: rgba(153, 153, 153, 1);
}

.icon-jiantou {
  font-size: 24rpx;
  color: #999;
  margin-left: 12rpx;
}

.tabList {
  width: 150rpx;
}

.resList {
  width: 600rpx;
  background-color: RGBA(250, 250, 250, 1);
}

.list {
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid rgba(0, 0, 0, 0.04);
  justify-content: center;
  font-size: 30rpx;
  font-family: Adobe Heiti Std R;
  font-weight: normal;
  color: rgba(102, 102, 102, 1);
}

.listActive::before {
  content: '';
  background: rgba(228, 84, 135, 1);
  left: 0;
  position: absolute;
  width: 7rpx;
  height: 74rpx;
}

.resItem {
  width: 100%;
  height: 157rpx;
  box-sizing: border-box;
  padding: 30rpx 60rpx;
  border-bottom: 1rpx solid rgba(0, 0, 0, 0.04);
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.left{
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.name {
  font-size: 30rpx;
  font-weight: normal;
  color: rgba(51, 51, 51, 1);
}

.name span {
  font-size: 22rpx;
  font-weight: normal;
  color: rgba(153, 153, 153, 1);
  margin-left: 17rpx;
}

.right {
  font-size: 24rpx;
  font-weight: normal;
  color: rgba(153, 153, 153, 1);
}

.res{
  font-size:30rpx;
font-family:Adobe Heiti Std R;
font-weight:normal;
color:rgba(102,102,102,1);
text-indent: 30rpx;
}