/* pages/lookCase/lookCase.wxss */

.wrap {
  height: 100%;
  box-sizing: border-box;
  padding: 25rpx 50rpx 50rpx 50rpx;
}

.title {
  font-size: 40rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
  text-align: center;
}

.noticeGroup {
  margin-top: 40rpx;
}

.name {
  font-size: 34rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
  margin-right: 35rpx;
}

.date {
  font-size: 34rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
  margin-top: 48rpx;
}

.mintitle {
  font-size: 34rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
  margin-top: 48rpx;
  text-indent: -7rpx;
  margin-bottom: 21rpx;
}

.res {
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
}
.resList{
  margin-top: 15rpx;
}
.otherGroup{
  display: flex;
  align-items: center;
  justify-content: space-around;
  margin-top: 50rpx;
}
.lookPort{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.icon{
  width: 128rpx;
  height: 148rpx;
  background-color: red;
}
.lookCase {
  width: 160rpx;
  height: 52rpx;
  box-sizing: border-box;
  border: 2rpx solid rgba(99, 223, 197, 1);
  border-radius: 26rpx;
  font-size: 24rpx;
  font-family: PingFang-SC-Bold;
  /* font-weight:bold; */
  color: rgba(99, 223, 197, 1);
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 25rpx;
}